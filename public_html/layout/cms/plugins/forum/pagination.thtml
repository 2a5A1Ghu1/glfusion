{# begin {templatelocation} #}
<ul class="uk-pagination">
	<!-- BEGIN datarow -->
{!if active}
	<li class="uk-active">
		<span>{page_str}</span>
{!else}
		<li class="{!if disabled}uk-disabled {!endif}{!if active}uk-active{!endif}">
			<a href="{page_link}">{page_str}</a>
{!endif}
	</li>
	<!-- END datarow -->
</ul>
{# end {templatelocation} #}