<!--[if IE 7]>
<link rel="stylesheet" type="text/css" href="{layout_url}/css/ie7.css" title="{theme}"{xthml}>
<![endif]-->
<style type="text/css">
#gl_moomenu,
#gl_moomenu ul,
#gl_moomenu ul.gl_moomenu {
background:{mbgcolor} {menu_bg};  /*Top Menu BG*/
}
#gl_moomenu a {
color:{mtext};  /*Top Menu Text*/
}
.gl_moomenu li:hover {
background:{mbghcolor} {menu_hover_bg};  /*Top Menu Hover*/
}
#gl_moomenu li {
float:{malign};  /*Top Menu alignment*/
}
#gl_moomenu li:hover a,
#gl_moomenu li.sfHover a,
#gl_moomenu li:hover a:hover,
#gl_moomenu li.sfHover a:hover {
color:{mhtext};  /*Top Menu Text Hover / Sub Menu Text*/
}
#gl_moomenu li:hover li:hover a:hover,
#gl_moomenu li.sfHover li.sfHover a:hover {
color:{shtext};  /*Sub Menu Text Hover*/
}
#gl_moomenu ul ul {
background:{sbgcolor};  /*Sub Menu BG*/
border-left:1px solid {sbhcolor};  /*Sub Menu Highlight*/
border-right:1px solid {sbscolor};  /*Sub Menu Shadow*/
}
#gl_moomenu li li {
border-top:1px solid {sbhcolor};  /*Sub Menu Highlight*/
border-bottom:1px solid {sbscolor};  /*Sub Menu Shadow*/
}
#gl_moomenu li li a.parent,
#gl_moomenu li li a.parent:hover {
background:{spimage};  /*image for sub menu parent menu - the blue orb*/
}
.gl_moomenu {float:{menualign};}
</style>
</head>
<body dir="{direction}" style="background:#000000;">
<div>
  <a name="top"></a>
  <a href="#content-start"> <!-- for screenreaders -->
  <img class="hidden" src="{layout_url}/images/speck.gif" alt="skip navigation"{xhtml}>
  </a>
</div>
