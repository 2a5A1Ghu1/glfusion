<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
<title>{page_title}{page_site_splitter}{site_name}</title>
<meta http-equiv="Content-Script-Type" content="text/javascript" />
<meta http-equiv="Content-Style-Type" content="text/css" />
<meta http-equiv="Content-Type" content="text/html; charset={charset}" />
{# <meta http-equiv="Pragma" content="no-cache" /> #}
{meta_data}
<link rel="SHORTCUT ICON" href="{layout_url}/images/favicon.ico" />
{# Load Stylesheets #}
<link rel="stylesheet" type="text/css" href="{style_cache_url}" title="{theme}" />
{css_links}
{feed_url}
{rel_links}
{# Load JavaScript #}
<script type="text/javascript" src="{js_cache_url}"></script>
{# Load JavaScript Links #}
{js_links}
{# Load plugin Header Code #}
{plg_headercode}
{# Load misc Header Vars #}
{header_vars}
<!--[if lte IE 6]>
<link rel="stylesheet" type="text/css" href="{layout_url}/ie6.css" title="{theme}" />
<script defer type="text/javascript" src="{site_url}/js/menu/ie6vertmenu.{js}"></script>
<![endif]-->
<!--[if IE 7]>
<link rel="stylesheet" type="text/css" href="{layout_url}/ie7.css" title="{theme}" />
<![endif]-->
<!--[if IE 8]>
<link rel="stylesheet" type="text/css" href="{layout_url}/ie7.css" title="{theme}" />
<script defer type="text/javascript" src="{site_url}/js/menu/ie6vertmenu.{js}"></script>
<![endif]-->
</head>
<body dir="{direction}" style="background:#000000;">
  <div>
    <a name="top"></a>
    <a href="#content-start"> {# for screenreaders #}
      <img class="hidden" src="{layout_url}/images/speck.gif" alt="skip navigation" />
    </a>
  </div>
  <div id="gl_container_fluid"> {# options: fixed | fluid #}
    <div id="gl_header">
      <div class="top-r-corner">
        <div class="top-l-corner">
          <div class="header-flag-n-search">
            <form method="get" action="{site_url}/search.php">
              <div>
                <input id="header-textbox" type="text" name="query" maxlength="255" value="{$LANG09[10]}" title="{$LANG09[10]}" onfocus="if (this.value=='Search')this.value='';" onblur="if(this.value=='')this.value='{$LANG09[10]}';" />
                <input type="hidden" name="type" value="all" />
                <input type="hidden" name="mode" value="search" />
                <input type="hidden" name="results" value="{num_search_results}" />
              </div>
            </form>
          </div> {# end of header-flag-n-search  #}
          {logo_block}
        </div>
      </div>
    </div> {# end of gl_header #}
{# start of gl_menu #}
{!if horizontal_menu}
    {horizontal_menu}
{!endif}
    <div id="gl_wrapper">
      <div id="{centercolumn}">
        <div class="welcomeanddate-text">
          <span class="gl_user-menu-right">
            {!!if !COM_isAnonUser() !!}
              <a href="{site_url}/usersettings.php?mode=edit">{$LANG01[48]}</a><br />
              <a href="{site_url}/users.php?mode=logout">{$LANG01[35]}</a>
            {!else!}
              {!!if !$_CONF['disable_new_user_registration'] !!}
                <a href="{site_url}/users.php?mode=new">{$LANG04[27]}</a><br />
              {!endif}
                <a href="{site_url}/users.php?mode=login">{$LANG01[58]}</a>
            {!endif}
          </span>
          <span class="gl_user-menu-left">
            <a href="{site_url}">{welcome_msg}</a><br />
            {datetime}
          </span>
        </div>
{# START OF CONTENT AREA #}
        <a name="content-start" id="content-start"></a> {# for screenreaders #}

{# SpeedLimit Warning #}
{# this will only display when the speedlimit variable is set #}

{!if speedlimit}
  <p style="width:100%;text-align:center;">
    <span class="alert pluginAlert" style="text-align:center;font-size:1.5em;">
  SpeedLimit Error - Your last {type} was {seconds} seconds ago.  This site requires at least {wait} seconds between entries.
  <br />
  Click <a href="javascript:history.back()">HERE</a> to return
  </span>
  </p>
{!endif}
{# end of SpeedLimit Warning #}

{# Error Handler output #}

{!if error_message}
<div id="errbox" style="font-weight:bold;width:95%;margin:10px;border:1px solid black;">
    <div style="padding:5px;color:#FFFFFF;background:#A70000;">
        <span class="floatright">{block_help}</span>
        Error Message
    </div>
    <div style="padding:5px 15px 15px 15px;border-top:3px solid black;background:#E7E7E7;">
     {error_message}
    </div>
</div>
{!endif}

{# end of error Handler output #}

{# warning messages go here #}

{# General Message Handler output #}

{!if general_message}

<script>
$(window).load( function() {
    alert('{general_message}');
});
</script>

<div id="msgbox" style="font-weight:bold;width:95%;margin:10px;border:1px solid black;">
    <div style="padding:5px;color:#FFFFFF;background:url({layout_url}/images/header-bg.png) #1A3955;">
        <span class="floatright">{block_help}</span>
        General Message
    </div>
    <div style="padding:5px 15px 15px 15px;border-top:3px solid black;background:#E7E7E7;">
     {general_message}
    </div>
</div>
<script type="text/javascript">
var fx = new Fx.Tween('msgbox', {
    duration : 1000,
    transition: Fx.Transitions.Quad.easeOut,
    onComplete : function() {
        $('msgbox').setStyle('display', 'none');
    }
});
fx.start.delay(4000, fx, ['opacity',0]);
</script>
{!endif}

{# end of General Message Handler output #}

        {content}
{# END OF CONTENT AREA #}
      </div> {# end of gl_content #}
    </div> {# end of gl_wrapper #}
{!if glfusion_blocks}
{# START OF LEFT BLOCKS #}
    <div id="gl_navigation" class="gl_blockstyle1"> {# gl_blockstyle1 - default title w/shading | gl_blockstyle2 - title w/out shading | gl_blockstyle3 - no title or shading | See style.css for more info #}
      <div class="block-columnpad">{glfusion_blocks}</div>
    </div>
{# END OF LEFT BLOCKS #}
{!endif}
{!if glfusion_rblocks}
{# START OF RIGHT BLOCKS #}
    <div id="gl_extra" class="gl_blockstyle1"> {# gl_blockstyle1 - default title w/shading | gl_blockstyle2 - title w/out shading | gl_blockstyle3 - no title or shading | See style.css for more info #}
      <div class="block-columnpad">{glfusion_rblocks}</div>
    </div>
{# END OF RIGHT BLOCKS #}
{!endif}
    <div id="gl_footer">
      <div class="bottom-r-corner">
        <div class="bottom-l-corner"></div>
      </div>
      {footer_menu}
      <div id="footer-copyright">
        {copyright_notice}&nbsp;&nbsp;&bull;&nbsp;&nbsp;<a href="{rss_url}">RSS</a>&nbsp;&nbsp;&bull;&nbsp;&nbsp;{execution_textandtime}&nbsp;{powered_by} <a href="{glfusion_url}">glFusion</a>
      </div> {# end of footer-copyright #}
    </div> {# end of gl_footer#}
  </div> {# end of gl_container #}
</body>
</html>
{# end {templatelocation} #}