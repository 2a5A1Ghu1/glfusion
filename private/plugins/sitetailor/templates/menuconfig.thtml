function hsv(a){
		var r=a[0],
		g=a[1],
		b=a[2],
		H,S,V;
		var c=Math.min(r,g,b);
		var d=Math.max(r,g,b);
		var e=d-c;
		V=d;
		if(!e){H=0;S=0}
		else{
			S=e/d;
			deltaR=(((d-r)/6)+(e/2))/e;
			deltaG=(((d-g)/6)+(e/2))/e;
			deltaB=(((d-b)/6)+(e/2))/e;
			if(r==d)H=deltaB-deltaG;
			else if(g==d)H=(1/3)+deltaR-deltaB;
			else if(b==d)H=(2/3)+deltaG-deltaR;
			if(H<0)H+=1;if(H>1)H-=1
		};
		return[H,S,V]
	}
