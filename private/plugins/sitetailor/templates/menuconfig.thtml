    <div class="glfform">

    <form method="post" action="{form_action}" id="mbconfig" enctype="multipart/form-data" >
    <input type="hidden" name="menu_id" id="menu_id" value="{menu_id}"{xhtml}>
    <input type="hidden" name="menutype" id="menutype" value="{menutype}"{xhtml}>
     <h1 style="margin:9px;">{$LANG_ST01[menu_properties]} {menu_name}</h1>
      <div>
        <label for="menuactive">{$LANG_ST01[active]}:</label>
        <input type="checkbox" id="menuactive" name="menuactive" value="1"{menuactive}{xhtml}>
      </div>
      <div>
        <label for="group">{$LANG_ST01[permission]}:</label>
        {group_select}
      </div>

	  <h1 style="margin:9px;display:{menu_alignment_show};">{$LANG_ST01[menu_alignment]}</h1>
      <div style="display:{menu_alignment_show};">
        <label>{$LANG_ST01[alignment_question]}:</label>
        &nbsp;&nbsp;&nbsp;{$LANG_ST01[align_left]}:&nbsp;<input type="radio" name="malign" value="1" {alignment_left_checked}{xhtml}>&nbsp;&nbsp;&nbsp;
        {$LANG_ST01[align_right]}:&nbsp;<input type="radio" name="malign" value="0" {alignment_right_checked}{xhtml}>
      </div>

      <h1 style="margin:9px;">{$LANG_ST01[menu_color_options]}</h1>

      <div style="display:{main_menu_bg_color_show};">
        <label>{lang_main_menu_bg_color}:</label>
        <input id="tmbgcolor" name="tmbgcolor" type="hidden" size="7" value="{tmbgcolor}" {xhtml}>&nbsp;
        <input type="text" name="tmbg_sample" id="tmbg_sample" size="7" maxlength="7" value="{tmbgcolor}" style="vertical-align:middle;background-color:{tmbgcolor};"{xhtml}>
        <img id="tmbgrb" src="{site_admin_url}/plugins/sitetailor/images/rainbow.png" alt="{$LANG_ST01[select_color]}" title="{$LANG_st01[select_color]}" width="16" height="16" style="vertical-align:middle;"{xhtml}>
        {!if show_warning}
        &nbsp;&nbsp;<span style="font-style:italic">{$LANG_ST01[not_used]}</span>
        {!endif}
      </div>

      <div style="display:{main_menu_hover_bg_color_show};">
        <label>{lang_main_menu_hover_bg_color}:</label>
