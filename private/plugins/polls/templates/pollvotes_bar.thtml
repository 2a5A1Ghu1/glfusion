{# begin {templatelocation} #}
<tr>
  <td class="poll_answer_text">{answer_text}</td>
  <td class="poll_answer_bar">
    <img src="{layout_url}/images/bar.gif" width="{bar_width}" height="10" style="height:10px;align:bottom;" alt="" /> {answer_num} ({answer_percent}%)
  </td>
</tr>
{!if remark_text}
<tr>
  <td class="uk-text-right" colspan="2">
    {remark_text}
  </td>
</tr>
{!endif}
{# end {templatelocation} #}
